<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DCOD3">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DCOD3">
        <activity
            android:name=".seller_act"
            android:exported="false" />
        <activity
            android:name=".Select_item"
            android:exported="false" />
        <activity
            android:name=".prodlist"
            android:exported="false" />
        <activity
            android:name=".buyprod"
            android:exported="false" />
        <activity
            android:name=".productList"
            android:exported="true" />
        <activity
            android:name=".addProduct"
            android:exported="true" />
        <activity
            android:name=".seller"
            android:exported="true" />
        <activity
            android:name=".ScanCode"
            android:exported="true" />
        <activity
            android:name=".reselleregister"
            android:exported="true" />
        <activity
            android:name=".resellerlogin"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>